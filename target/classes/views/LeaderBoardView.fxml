<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane id="leaderboardbackground" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.LeaderBoardController">
   <center>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="375.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <top>
            <Label text="LeaderBoard" textFill="WHITE" BorderPane.alignment="CENTER">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font></Label>
         </top>
         <center>
            <VBox maxHeight="-Infinity" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <ListView fx:id="leaderboardView" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </center>
         <bottom>
            <Button id="backbutton" mnemonicParsing="false" onAction="#onClickBack" text="Back" translateY="-20.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
