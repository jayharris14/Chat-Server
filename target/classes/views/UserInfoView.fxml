<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.UserInfoController">
   <center>
      <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="375.0" prefWidth="300.0" BorderPane.alignment="CENTER">
         <top>
            <Label text="Edit Info" BorderPane.alignment="CENTER" />
         </top>
         <center>
            <VBox maxHeight="-Infinity" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Change Username" />
                        <TextField id="usernamelabel" fx:id="usernameLabel" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Change Password" />
                        <TextField id="passwordlabel" fx:id="passwordLabel" translateX="5.0" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="110.0" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Change Name" />
                        <TextField id="namelabel" fx:id="nameLabel" translateX="29.0" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label text="Profile Data" />
                        <TextField id="datalabel" fx:id="profileLabel" translateX="45.0" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" />
               </children>
            </VBox>
         </center>
         <bottom>
            <Button id="submitbutton" mnemonicParsing="false" onAction="#onClickSubmit" text="Submit" translateY="-20.0" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
